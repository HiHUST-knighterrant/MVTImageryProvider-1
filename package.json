  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hongfaqiu/MVTImageryProvider.git"
  },
  "keywords": [
    "Mapbox Style",
    "Vector Tile",
    "Cesium"
  ],
  "license": "MIT",
  "peerDependencies": {
    "cesium": "*"
  },
  "devDependencies": {
    "cesium": "^1.90.0",
    "microbundle": "^0.14.2"
  },
  "dependencies": {
    "pbf-basic-render": "^0.0.2"
  },
  "files": [
    "dist",
    "README.md"
  ]
